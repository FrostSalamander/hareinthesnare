textures/darkmod/wood/signs/sign_shield_hare
{   qer_editorimage	textures/darkmod/wood/signs/sign_shield_hare
	wood

	diffusemap  textures/darkmod/wood/signs/sign_shield_hare
	bumpmap     _flat
	{   if ( parm11 > 0 )
		blend       gl_dst_color, gl_one
		map         _white
		rgb         0.40 * parm11
	}
	{   if ( parm11 > 0 )
		blend       add
		map         textures/darkmod/wood/signs/sign_shield_hare
		rgb         0.15 * parm11
	}
}

teddy_bear_model
{
	surftype15
	noselfshadow

	{
		blend	diffusemap
		map		textures/darkmod/map_specific/teddy_bear_diffuse
		alphaTest 0.5
	}

	bumpmap		textures/darkmod/map_specific/teddy_bear_bump

	{
		if ( parm11 > 0 )
		blend       gl_dst_color, gl_one
		map         _white
		rgb         0.40 * parm11
	}
	{
		if ( parm11 > 0 )
		blend       add
		map         textures/darkmod/map_specific/teddy_bear_diffuse
		rgb         0.15 * parm11
	}
}